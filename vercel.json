{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://vercel-mr-clement-pos-backend.vercel.app"
  },
  "outputDirectory": "dist"
}